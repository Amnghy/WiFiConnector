/* pages/success/success.wxss */
view {
  width: 100vw;
  height: 80vh;
  display: block;
  align-items: center;
  justify-content: center;
}
view,text{
  word-break:break-all;
}
.wrap-top{
  margin-top: 60%;
  display: flex;
  align-items: center;
  justify-content: center;
}
.wrap-top text{
  color: #006400; /* 设置颜色为蓝色 */
  font-weight: bold; /* 设置字体为加粗 */
  font-size: 30px; /* 设置字体大小为24像素 */
}
.wrap-div{
  margin-top: 10%;
  display: flex;
  align-items: center;
  justify-content: center;
}
.wrap-div text{
  white-space: nowrap;
  color: 	#8B008B;
  font-weight: bold;
  font-size: 30px;
}